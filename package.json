{
  "name": "duo-autoprefixer",
  "version": "1.0.0",
  "description": "autoprefixer plugin for duo",
  "repository": "duojs/autoprefixer",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint *.js & jscs *.js",
    "test": "tape test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/duojs/autoprefixer/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "duo",
    "duoplugin",
    "css",
    "style",
    "vendor",
    "prefix",
    "prefixer",
    "autoprefixer",
    "cross-browser",
    "postprocessor",
    "postprocess"
  ],
  "dependencies": {
    "autoprefixer-core": "^5.0.0",
    "tryit": "^1.0.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "duo": "^0.9.1",
    "eslint": "^0.11.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "rimraf": "^2.2.8",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
